{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "preLaunchTask": "build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/api.ts",
            "outFiles": [
                "${workspaceFolder}/dist/*.js"
            ],
            "env": {
                "OPENAI_API_KEY": "ENTER YOUR KEY"
            }
        }
    ]
}